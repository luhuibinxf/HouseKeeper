<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.feicui.app.phone">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/life"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LeadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LogoActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.MobileActivity" />
        <activity android:name=".activity.PhoneMobileActivity" />
        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".activity.SoftwareActivity" />
    </application>

</manifest>